{
  "name": "@craftercms/studio-guest",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "craftercms": {
    "id": "craftercms.guest"
  },
  "dependencies": {
    "@craftercms/classes": "1.2.4",
    "@craftercms/content": "1.2.4",
    "@craftercms/search": "1.2.4",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/jquery": "^3.3.32",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "jquery": "^3.5.0",
    "js-cookie": "^2.2.1",
    "jss": "^10.0.3",
    "jss-preset-default": "^10.0.0",
    "lorem-ipsum": "^2.0.3",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.4",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "build": "run-s build:install build:app",
    "build:install": "yarn",
    "build:app": "rollup -c",
    "watch": "rollup -c -w",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "compile": "tsc --build tsconfig.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/",
              "#"
            ]
          },
          "block": {
            "balanced": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-typescript": "^7.7.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/tinymce": "^4.5.24",
    "rollup": "^2.3.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "yargs": "^15.3.1"
  },
  "bic": {
    "skip": [
      "build",
      "build_tsc",
      "README.md",
      "CHANGE_LOG.md"
    ]
  }
}
